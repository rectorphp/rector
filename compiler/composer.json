{
    "name": "rector/rector-compiler",
    "description": "PHAR Compiler for Rector",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/console": "^4.4|^5.0",
        "symfony/process": "^4.4|^5.0",
        "symfony/filesystem": "^4.4|^5.0",
        "nette/utils": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Rector\\Compiler\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "extra": {
        "cleaner-ignore": {
            "phpstan/phpstan": ["conf"]
        }
    }
}
