{
    "name": "symplify\/simple-php-doc-parser",
    "description": "Service integration of phpstan\/phpdoc-parser, with few extra goodies for practical simple use",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "phpstan\/phpdoc-parser": "^0.5",
        "symfony\/dependency-injection": "^5.3",
        "symfony\/config": "^5.3",
        "symfony\/http-kernel": "^5.3",
        "symplify\/package-builder": "^9.4.1"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5",
        "symplify\/easy-testing": "^9.4.1"
    },
    "autoload": {
        "psr-4": {
            "RectorPrefix20210630\\Symplify\\SimplePhpDocParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RectorPrefix20210630\\Symplify\\SimplePhpDocParser\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}