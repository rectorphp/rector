{
    "name": "symplify\/console-color-diff",
    "description": "Package to print diffs in console with colors",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette\/utils": "^3.2",
        "symfony\/console": "^5.3.x-dev",
        "sebastian\/diff": "^4.0",
        "symfony\/dependency-injection": "^5.3.x-dev",
        "symfony\/http-kernel": "^5.3",
        "symplify\/package-builder": "^9.4.20"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RectorPrefix20210710\\Symplify\\ConsoleColorDiff\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RectorPrefix20210710\\Symplify\\ConsoleColorDiff\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}