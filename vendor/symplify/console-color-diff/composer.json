{
    "name": "symplify\/console-color-diff",
    "description": "Package to print diffs in console with colors",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "nette\/utils": "^3.2",
        "symfony\/console": "^4.4|^5.3",
        "sebastian\/diff": "^4.0",
        "symfony\/dependency-injection": "^5.3",
        "symfony\/http-kernel": "^4.4|^5.3",
        "symplify\/package-builder": "^9.3.26"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RectorPrefix20210625\\Symplify\\ConsoleColorDiff\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RectorPrefix20210625\\Symplify\\ConsoleColorDiff\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}