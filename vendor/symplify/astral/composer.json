{
    "name": "symplify\/astral",
    "description": "Toolking for smart daily work with AST",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette\/utils": "^3.2",
        "symfony\/dependency-injection": "^5.3.x-dev",
        "symplify\/autowire-array-parameter": "^9.4.20",
        "symfony\/http-kernel": "^5.3",
        "nikic\/php-parser": "^4.11",
        "symplify\/package-builder": "^9.4.20"
    },
    "require-dev": {
        "symplify\/easy-testing": "^9.4.20",
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RectorPrefix20210710\\Symplify\\Astral\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RectorPrefix20210710\\Symplify\\Astral\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}