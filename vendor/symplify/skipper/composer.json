{
    "name": "symplify\/skipper",
    "description": "Skip files by rule class, directory, file or fnmatch",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "nette\/utils": "^3.2",
        "symfony\/config": "^4.4|^5.3",
        "symfony\/dependency-injection": "^5.3",
        "symfony\/finder": "^4.4|^5.3",
        "symfony\/filesystem": "^4.4|^5.3",
        "symplify\/package-builder": "^9.3.26",
        "symplify\/symplify-kernel": "^9.3.26",
        "symplify\/smart-file-system": "^9.3.26"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RectorPrefix20210626\\Symplify\\Skipper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RectorPrefix20210626\\Symplify\\Skipper\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}