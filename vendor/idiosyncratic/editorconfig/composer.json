{
    "name": "idiosyncratic\/editorconfig",
    "description": "PHP implementation of EditorConfig",
    "type": "library",
    "license": "ISC",
    "authors": [
        {
            "name": "Jason Silkey",
            "email": "jason@jasonsilkey.com"
        }
    ],
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "idiosyncratic\/coding-standard": "^2.0",
        "php-parallel-lint\/php-console-highlighter": "^0.5",
        "php-parallel-lint\/php-parallel-lint": "^1.3",
        "phploc\/phploc": "^7.0",
        "phpstan\/phpstan": "^0.12",
        "phpunit\/phpunit": "^9.5",
        "sebastian\/phpcpd": "^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "discard-changes": true,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "RectorPrefix20211231\\Idiosyncratic\\EditorConfig\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RectorPrefix20211231\\Idiosyncratic\\EditorConfig\\": "tests"
        }
    }
}