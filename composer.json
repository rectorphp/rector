            "Rector\\Php70\\Tests\\": "packages/Php70/tests",
            "Rector\\Php71\\Tests\\": "packages/Php71/tests",
            "Rector\\Php72\\Tests\\": "packages/Php72/tests",
            "Rector\\Php73\\Tests\\": "packages/Php73/tests",
